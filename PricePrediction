_data_cache'\nos.makedirs(DATA_DIR, exist_ok=True)\n\nclass DataCache:\n    \"\"\"Handles data persistence and caching\"\"\"\n    \n    def __init__(self, cache_dir=DATA_DIR):\n        self.cache_dir = cache_dir\n        self.cache_file = os.path.join(cache_dir, 'stock_data_cache.pkl')\n        self.metadata_file = os.path.join(cache_dir, 'cache_metadata.json')\n    \n    def save_data(self, stock_data: Dict[str, pd.DataFrame], metadata: Dict = None):\n        \"\"\"Save stock data to cache with metadata\"\"\"\n        try:\n            # Save the data\n            with open(self.cache_file, 'wb') as f:\n                pickle.dump(stock_data, f)\n            \n            # Save metadata\n            if metadata is None:\n                metadata = {}\n            \n            metadata.update({\n                'last_update': datetime.now().isoformat(),\n                'stocks': list(stock_data.keys()),\n                'data_points': {stock: len(df) for stock, df in stock_data.items()}\n            })\n            \n            with open(self.metadata_file, 'w') as f:\n                json.dump(metadata, f, indent=2)\n            \n            print(f\"âœ… Data cached successfully at {self.cache_file}\")\n            return True\n            \n        except Exception as e:\n            print(f\"âŒ Failed to save cache: {str(e)}\")\n            return False\n    \n    def load_data(self) -> Tuple[Optional[Dict[str, pd.DataFrame]], Optional[Dict]]:\n        \"\"\"Load cached data and metadata\"\"\"\n        try:\n            # Check if cache exists\n            if not os.path.exists(self.cache_file):\n                print(\"ðŸ“­ No cached data found\")\n                return None, None\n            \n            # Load data\n            with open(self.cache_file, 'rb') as f:\n                stock_data = pickle.load(f)\n            \n            # Load metadata\n            metadata = {}\n            if os.path.exists(self.metadata_file):\n                with open(self.metadata_file, 'r') as f:\n                    metadata = json.load(f)\n            \n            # Display cache info\n            if metadata:\n                last_update = metadata.get('last_update', 'Unknown')\n                print(f\"ðŸ“¦ Loaded cached data from: {last_update}\")\n                print(f\"   Stocks: {', '.join(metadata.get('stocks', []))}\")\n            \n            return stock_data, metadata\n            \n        except Exception as e:\n            print(f\"âŒ Failed to load cache: {str(e)}\")\n            return None, None\n    \n    def get_cache_age_hours(self) -> Optional[float]:\n        \"\"\"Get age of cache in hours\"\"\"\n        try:\n            if os.path.exists(self.metadata_file):\n                with open(self.metadata_file, 'r') as f:\n                    metadata = json.load(f)\n                last_update = datetime.fromisoformat(metadata['last_update'])\n                age = (datetime.now() - last_update).total_seconds() / 3600\n                return age\n            return None\n        except:\n            return None\n\nclass StockConfig:\n    \"\"\"Configuration with data caching support\"\"\"\n    \n    # NSE tickers with alternatives\n    STOCK_SYMBOLS = {\n        'RELIANCE': ['RELIANCE.NS', 'RELIANCE.BO'],\n        'TCS': ['TCS.NS', 'TCS.BO'],\n        'HDFCBANK': ['HDFCBANK.NS', 'HDFCBANK.BO'],\n        'INFY': ['INFY.NS', 'INFY.BO'],\n        'ITC': ['ITC.NS', 'ITC.BO']\n    }\n    \n    # Cache settings\n    CACHE_MAX_AGE_HOURS = 24  # Refresh cache if older than 24 hours\n    ALWAYS_TRY_LIVE_FIRST = True  # Try fetching live data even if cache exists\n    \n    # Model parameters\n    LOOKBACK_DAYS = 30\n    TRAIN_TEST_SPLIT = 0.8\n    MIN_DATA_POINTS = 100\n\nconfig = StockConfig()\n\nclass SmartDataCollector:\n    \"\"\"Data collector with intelligent caching\"\"\"\n    \n    def __init__(self):\n        self.cache = DataCache()\n        self.successful_tickers = []\n        self.failed_tickers = []\n        self.used_cache = []\n    \n    def try_fetch_real_data(self, symbols_list, stock_name):\n        \"\"\"Try fetching real data from yfinance\"\"\"\n        if not YFINANCE_AVAILABLE:\n            return None\n        \n        for symbol in symbols_list:\n            print(f\"  ðŸ” Trying {symbol}...\")\n            try:\n                # Try using Ticker object\n                ticker = yf.Ticker(symbol)\n                data = ticker.history(period=\"2y\")\n                \n                if not data.empty and len(data) > config.MIN_DATA_POINTS:\n                    print(f\"  âœ… Success with {symbol}: {len(data)} rows\")\n                    data.reset_index(inplace=True)\n                    data['Symbol'] = stock_name\n                    return data\n                \n                # Try download method\n                end_date = datetime.now()\n                start_date = end_date - timedelta(days=730)\n                \n                data = yf.download(\n                    symbol,\n                    start=start_date.strftime('%Y-%m-%d'),\n                    end=end_date.strftime('%Y-%m-%d'),\n                    progress=False\n                )\n                \n                if not data.empty and len(data) > config.MIN_DATA_POINTS:\n                    print(f\"  âœ… Success with download method: {len(data)} rows\")\n                    data.reset_index(inplace=True)\n                    data['Symbol'] = stock_name\n                    return data\n                \n            except Exception as e:\n                print(f\"  âŒ Failed {symbol}: {str(e)[:50]}...\")\n                continue\n            \n            time.sleep(1)  # Rate limiting\n        \n        return None\n    \n    def collect_stock_data(self):\n        \"\"\"Collect data with intelligent caching strategy\"\"\"\n        print(\"ðŸš€ Starting smart data collection with caching\")\n        print(\"=\" * 60)\n        \n        # Load cached data first\n        cached_data, cache_metadata = self.cache.load_data()\n        cache_age = self.cache.get_cache_age_hours()\n        \n        if cache_age is not None:\n            print(f\"ðŸ“Š Cache age: {cache_age:.1f} hours\")\n        \n        stock_data = {}\n        fresh_data = {}\n        \n        # Determine if we should try live data\n        try_live = YFINANCE_AVAILABLE and (\n            config.ALWAYS_TRY_LIVE_FIRST or \n            cached_data is None or \n            (cache_age and cache_age > config.CACHE_MAX_AGE_HOURS)\n        )\n        \n        if try_live:\n            print(\"\\nðŸŒ Attempting to fetch fresh data from yfinance...\")\n            \n            for stock_name, symbols in config.STOCK_SYMBOLS.items():\n                print(f\"\\nðŸ“ˆ Fetching {stock_name}\")\n                \n                # Try live data\n                data = self.try_fetch_real_data(symbols, stock_name)\n                \n                if data is not None:\n                    fresh_data[stock_name] = data\n                    stock_data[stock_name] = data\n                    self.successful_tickers.append(stock_name)\n                else:\n                    # Fall back to cached data\n                    if cached_data and stock_name in cached_data:\n                        print(f\"  ðŸ“¦ Using cached data for {stock_name}\")\n                        stock_data[stock_name] = cached_data[stock_name]\n                        self.used_cache.append(stock_name)\n                    else:\n                        print(f\"  âŒ No data available for {stock_name}\")\n                        self.failed_tickers.append(stock_name)\n            \n            # Save fresh data to cache if any was fetched\n            if fresh_data:\n                print(f\"\\nðŸ’¾ Saving {len(fresh_data)} fresh datasets to cache...\")\n                # Merge with existing cache\n                if cached_data:\n                    for stock, data in fresh_data.items():\n                        cached_data[stock] = data\n                    self.cache.save_data(cached_data)\n                else:\n                    self.cache.save_data(fresh_data)\n        \n        else:\n            # Use only cached data\n            print(\"\\nðŸ“¦ Using cached data (live fetch disabled or unavailable)\")\n            if cached_data:\n                stock_data = cached_data\n                self.used_cache = list(cached_data.keys())\n            else:\n                print(\"âŒ No cached data available and cannot fetch live data\")\n        \n        # Summary\n        print(f\"\\nðŸ“Š Data collection summary:\")\n        print(f\"  âœ… Fresh data: {len(self.successful_tickers)} stocks\")\n        print(f\"  ðŸ“¦ Cached data: {len(self.used_cache)} stocks\")\n        print(f\"  âŒ Failed: {len(self.failed_tickers)} stocks\")\n        print(f\"  ðŸ“ˆ Total available: {len(stock_data)} stocks\")\n        \n        return stock_data\n\nclass ModernVisualizer:\n    \"\"\"Create modern visualizations using matplotlib (with Plotly-style if available)\"\"\"\n    \n    def __init__(self):\n        self.colors = {\n            'primary': '#1f77b4',\n            'secondary': '#ff7f0e',\n            'success': '#2ca02c',\n            'danger': '#d62728',\n            'warning': '#ff9800',\n            'info': '#17a2b8'\n        }\n    \n    def compare_model_performance(self, training_results):\n        \"\"\"\n        Model Evaluation: Performance Comparison Across Algorithms\n        Critical for demonstrating systematic model selection methodology\n        \"\"\"\n        print(f\"ðŸ“Š Generating ML model performance comparison\")\n        \n        # Extract ACTUAL performance metrics from training results\n        stocks = []\n        rf_rmse = []\n        lr_rmse = []\n        rf_r2 = []\n        lr_r2 = []\n        \n        for stock_name, result in training_results.items():\n            if result and 'all_results' in result:\n                stocks.append(stock_name)\n                \n                # Extract actual results from both models\n                all_results = result['all_results']\n                \n                # Get Random Forest results\n                if 'Random_Forest' in all_results:\n                    rf_rmse.append(all_results['Random_Forest']['rmse'])\n                    rf_r2.append(all_results['Random_Forest']['r2'])\n                else:\n                    rf_rmse.append(0)\n                    rf_r2.append(0)\n                \n                # Get Linear Regression results\n                if 'Linear_Regression' in all_results:\n                    lr_rmse.append(all_results['Linear_Regression']['rmse'])\n                    lr_r2.append(all_results['Linear_Regression']['r2'])\n                else:\n                    lr_rmse.append(0)\n                    lr_r2.append(0)\n        \n        if not stocks:\n            print(\"âŒ No model results available for comparison\")\n            return None\n        \n        # Create comparative visualization\n        fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(16, 8))\n        \n        # RMSE Comparison (Lower indicates better performance)\n        x = np.arange(len(stocks))\n        width = 0.35\n        \n        bars1 = ax1.bar(x - width/2, rf_rmse, width, label='Random Forest', \n                       color='#ff7f0e', alpha=0.8, edgecolor='black', linewidth=1)\n        bars2 = ax1.bar(x + width/2, lr_rmse, width, label='Linear Regression', \n                       color='#2ca02c', alpha=0.8, edgecolor='black', linewidth=1)\n        \n        # Add performance values on bars\n        for bars in [bars1, bars2]:\n            for bar in bars:\n                height = bar.get_height()\n                if height > 0:  # Only add label if height is valid\n                    ax1.text(bar.get_x() + bar.get_width()/2., height + height*0.01,\n                            f'{height:.2f}', ha='center', va='bottom', fontweight='bold')\n        \n        ax1.set_title('Algorithm Performance: RMSE Comparison\\n(Lower Values Indicate Better Accuracy)', \n                     fontsize=14, fontweight='bold')\n        ax1.set_ylabel('RMSE (Root Mean Square Error)', fontsize=12, fontweight='bold')\n        ax1.set_xlabel('Stock Symbols', fontsize=12, fontweight='bold')\n        ax1.set_xticks(x)\n        ax1.set_xticklabels(stocks, rotation=45)\n        ax1.legend(fontsize=11)\n        ax1.grid(True, alpha=0.3)\n        \n        # RÂ² Score Comparison (Higher indicates better fit)\n        bars3 = ax2.bar(x - width/2, rf_r2, width, label='Random Forest', \n                       color='#ff7f0e', alpha=0.8, edgecolor='black', linewidth=1)\n        bars4 = ax2.bar(x + width/2, lr_r2, width, label='Linear Regression', \n                       color='#2ca02c', alpha=0.8, edgecolor='black', linewidth=1)\n        \n        # Add RÂ² values\n        for bars in [bars3, bars4]:\n            for bar in bars:\n                height = bar.get_height()\n                if height > 0:  # Only add label if height is valid\n                    ax2.text(bar.get_x() + bar.get_width()/2., height + height*0.002,\n                            f'{height:.3f}', ha='center', va='bottom', fontweight='bold')\n        \n        ax2.set_title('Algorithm Performance: RÂ² Score Comparison\\n(Higher Values Indicate Better Model Fit)', \n                     fontsize=14, fontweight='bold')\n        ax2.set_ylabel('RÂ² Score (Coefficient of Determination)', fontsize=12, fontweight='bold')\n        ax2.set_xlabel('Stock Symbols', fontsize=12, fontweight='bold')\n        ax2.set_xticks(x)\n        ax2.set_xticklabels(stocks, rotation=45)\n        ax2.legend(fontsize=11)\n        ax2.grid(True, alpha=0.3)\n        \n        # Set appropriate y-limits for RÂ² score based on actual data\n        if rf_r2 and lr_r2:\n            min_r2 = min(min(rf_r2), min(lr_r2))\n            max_r2 = max(max(rf_r2), max(lr_r2))\n            y_range = max_r2 - min_r2\n            ax2.set_ylim(min_r2 - y_range*0.05, max_r2 + y_range*0.05)\n        \n        plt.suptitle('Machine Learning Model Performance Analysis', \n                    fontsize=16, fontweight='bold', y=0.98)\n        plt.tight_layout()\n        \n        # Save comparison chart\n        filename = 'ModelPerformance_AlgorithmComparison.png'\n        plt.savefig(filename, dpi=300, bbox_inches='tight', facecolor='white')\n        print(f\"âœ… Model performance comparison saved as: {filename}\")\n        \n        plt.show()\n        return fig\n    \n    def create_7day_prediction_chart(self, stock_name, df, predictions):\n        \"\"\"Create a detailed 7-day prediction chart\"\"\"\n        if predictions is None or len(predictions) == 0:\n            return None\n        \n        # Create figure\n        fig, (ax1, ax2) = plt.subplots(2, 1, figsize=(14, 10), height_ratios=[3, 1])\n        fig.suptitle(f'{stock_name} - Detailed 7-Day Price Predictions', fontsize=18, fontweight='bold')\n        \n        # Prepare data\n        df = df.copy()\n        df['Date'] = pd.to_datetime(df['Date'])\n        recent_history = df.tail(30)  # Last 30 days for context\n        current_price = df['Close'].iloc[-1]\n        \n        # Plot 1: Time series with predictions\n        # Historical data\n        ax1.plot(recent_history['Date'], recent_history['Close'], \n                 color=self.colors['primary'], linewidth=2.5, label='Historical Price', marker='o', markersize=4)\n        \n        # Connection line\n        ax1.plot([recent_history['Date'].iloc[-1], predictions['Date'].iloc[0]], \n                 [current_price, predictions['Predicted_Price'].iloc[0]], \n                 '--', color='gray', alpha=0.5, linewidth=2)\n        \n        # Predictions with markers for each day\n        ax1.plot(predictions['Date'], predictions['Predicted_Price'], \n                 color=self.colors['danger'], linewidth=3, label='7-Day Predictions', \n                 marker='s', markersize=10, markeredgecolor='black', markeredgewidth=1)\n        \n        # Add value labels for each prediction\n        for idx, row in predictions.iterrows():\n            ax1.annotate(f'â‚¹{row[\"Predicted_Price\"]:.1f}', \n                        xy=(row['Date'], row['Predicted_Price']),\n                        xytext=(0, 10), textcoords='offset points',\n                        ha='center', fontsize=10, fontweight='bold',\n                        bbox=dict(boxstyle='round,pad=0.3', facecolor='yellow', alpha=0.7))\n        \n        # Add current price line\n        ax1.axhline(y=current_price, color='gray', linestyle=':', alpha=0.7, linewidth=2)\n        ax1.text(recent_history['Date'].iloc[-10], current_price + current_price*0.01, \n                f'Current: â‚¹{current_price:.2f}', fontsize=11, fontweight='bold',\n                bbox=dict(boxstyle='round,pad=0.3', facecolor='lightblue', alpha=0.8))\n        \n        # Formatting\n        ax1.set_ylabel('Price (â‚¹)', fontsize=14, fontweight='bold')\n        ax1.legend(loc='upper left', fontsize=12)\n        ax1.grid(True, alpha=0.3)\n        ax1.set_xlim(recent_history['Date'].iloc[0], predictions['Date'].iloc[-1] + timedelta(days=1))\n        \n        # Plot 2: Daily change bars\n        daily_changes = []\n        daily_change_pct = []\n        \n        # Calculate daily changes\n        prev_price = current_price\n        for idx, row in predictions.iterrows():\n            change = row['Predicted_Price'] - prev_price\n            change_pct = (change / prev_price) * 100\n            daily_changes.append(change)\n            daily_change_pct.append(change_pct)\n            prev_price = row['Predicted_Price']\n        \n        # Create bar chart\n        days = [f'Day {i+1}' for i in range(len(predictions))]\n        colors = [self.colors['success'] if x > 0 else self.colors['danger'] for x in daily_changes]\n        \n        bars = ax2.bar(days, daily_change_pct, color=colors, alpha=0.7, edgecolor='black', linewidth=1)\n        \n        # Add value labels on bars\n        for bar, value in zip(bars, daily_change_pct):\n            height = bar.get_height()\n            ax2.text(bar.get_x() + bar.get_width()/2., height + (0.05 if height > 0 else -0.15),\n                    f'{value:.2f}%', ha='center', va='bottom' if height > 0 else 'top',\n                    fontweight='bold', fontsize=10)\n        \n        ax2.axhline(y=0, color='black', linewidth=1)\n        ax2.set_ylabel('Daily Change (%)', fontsize=14, fontweight='bold')\n        ax2.set_xlabel('Prediction Days', fontsize=14, fontweight='bold')\n        ax2.set_title('Daily Price Change Percentage', fontsize=14)\n        ax2.grid(True, alpha=0.3)\n        \n        # Add summary box\n        total_change = predictions['Predicted_Price'].iloc[-1] - current_price\n        total_change_pct = (total_change / current_price) * 100\n        avg_price = predictions['Predicted_Price'].mean()\n        \n        summary_text = f'7-Day Summary:\\n'\n        summary_text += f'Current: â‚¹{current_price:.2f}\\n'\n        summary_text += f'Final: â‚¹{predictions[\"Predicted_Price\"].iloc[-1]:.2f}\\n'\n        summary_text += f'Change: {total_change_pct:+.2f}%\\n'\n        summary_text += f'Avg Price: â‚¹{avg_price:.2f}'\n        \n        ax1.text(0.02, 0.98, summary_text, transform=ax1.transAxes,\n                fontsize=11, verticalalignment='top',\n                bbox=dict(boxstyle='round', facecolor='wheat', alpha=0.8))\n        \n        plt.tight_layout()\n        \n        # Save and display\n        filename = f'{stock_name}_7day_predictions.png'\n        plt.savefig(filename, dpi=300, bbox_inches='tight', facecolor='white')\n        print(f\"  ðŸ’¾ 7-day prediction chart saved as: {filename}\")\n        plt.show()\n        \n        return fig\n    \n    def create_modern_price_chart(self, stock_name, df, predictions=None):\n        \"\"\"Create a modern-looking price chart\"\"\"\n        # Set up the style\n        plt.style.use('seaborn-v0_8-darkgrid')\n        \n        # Prepare data\n        df = df.copy()\n        df['Date'] = pd.to_datetime(df['Date'])\n        recent_data = df.tail(60)  # Last 60 days\n        \n        # Create figure with subplots\n        fig = plt.figure(figsize=(16, 10))\n        \n        # Main price chart\n        ax1 = plt.subplot(2, 1, 1)\n        \n        # Plot candlestick-style data\n        up = recent_data[recent_data.Close >= recent_data.Open]\n        down = recent_data[recent_data.Close < recent_data.Open]\n        \n        # Plot price line\n        ax1.plot(recent_data['Date'], recent_data['Close'], \n                 color=self.colors['primary'], linewidth=2.5, label='Close Price')\n        \n        # Add moving averages\n        ma_20 = recent_data['Close'].rolling(window=20).mean()\n        ax1.plot(recent_data['Date'], ma_20, \n                 color=self.colors['secondary'], linewidth=2, label='MA 20', alpha=0.8)\n        \n        # Add predictions if available\n        if predictions is not None:\n            # Connect historical to prediction\n            ax1.plot([recent_data['Date'].iloc[-1], predictions['Date'].iloc[0]], \n                     [recent_data['Close'].iloc[-1], predictions['Predicted_Price'].iloc[0]], \n                     '--', color=self.colors['danger'], alpha=0.5)\n            \n            # Plot predictions\n            ax1.plot(predictions['Date'], predictions['Predicted_Price'], \n                     color=self.colors['danger'], linewidth=3, \n                     label='7-Day Predictions', marker='o', markersize=8)\n            \n            # Add shaded area for prediction period\n            ax1.axvspan(predictions['Date'].iloc[0], predictions['Date'].iloc[-1], \n                        alpha=0.1, color=self.colors['danger'])\n        \n        # Formatting\n        ax1.set_title(f'{stock_name} - Price Analysis & Predictions', \n                      fontsize=18, fontweight='bold', pad=20)\n        ax1.set_ylabel('Price (â‚¹)', fontsize=14, fontweight='bold')\n        ax1.legend(loc='upper left', fontsize=12)\n        ax1.grid(True, alpha=0.3)\n        \n        # Volume subplot\n        ax2 = plt.subplot(2, 1, 2, sharex=ax1)\n        ax2.bar(recent_data['Date'], recent_data['Volume'], \n                color=self.colors['info'], alpha=0.5, label='Volume')\n        ax2.set_ylabel('Volume', fontsize=14, fontweight='bold')\n        ax2.set_xlabel('Date', fontsize=14, fontweight='bold')\n        ax2.grid(True, alpha=0.3)\n        \n        # Format dates\n        fig.autofmt_xdate()\n        \n        plt.tight_layout()\n        \n        # Save and display\n        filename = f'{stock_name}_modern_analysis.png'\n        plt.savefig(filename, dpi=300, bbox_inches='tight', facecolor='white')\n        print(f\"  ðŸ’¾ Chart saved as: {filename}\")\n        plt.show()  # Display the chart\n        \n        return fig\n    \n    def create_performance_dashboard(self, stock_name, df, trainer):\n        \"\"\"Create a comprehensive performance dashboard\"\"\"\n        if stock_name not in trainer.models:\n            return None\n        \n        try:\n            # Get model and prepare test data\n            model = trainer.models[stock_name]\n            scaler = trainer.scalers[stock_name]\n            feature_cols = trainer.feature_columns\n            \n            # Split data\n            split_idx = int(len(df) * 0.8)\n            test_data = df.iloc[split_idx:].copy()\n            \n            # Prepare features\n            X_test = test_data[feature_cols].fillna(test_data[feature_cols].mean())\n            y_test = test_data['Close'].values\n            \n            # Get predictions\n            X_test_scaled = scaler.transform(X_test)\n            y_pred = model.predict(X_test_scaled)\n            \n            # Calculate metrics\n            rmse = np.sqrt(mean_squared_error(y_test, y_pred))\n            mae = mean_absolute_error(y_test, y_pred)\n            r2 = r2_score(y_test, y_pred)\n            \n            # Create figure\n            fig, axes = plt.subplots(2, 2, figsize=(16, 12))\n            fig.suptitle(f'{stock_name} - Model Performance Dashboard\\nRMSE: {rmse:.2f} | MAE: {mae:.2f} | RÂ²: {r2:.3f}', \n                         fontsize=18, fontweight='bold')\n            \n            # 1. Actual vs Predicted\n            ax1 = axes[0, 0]\n            ax1.scatter(y_test, y_pred, alpha=0.6, color=self.colors['primary'], s=50)\n            min_val = min(y_test.min(), y_pred.min())\n            max_val = max(y_test.max(), y_pred.max())\n            ax1.plot([min_val, max_val], [min_val, max_val], 'r--', linewidth=2)\n            ax1.set_xlabel('Actual Price (â‚¹)', fontsize=12)\n            ax1.set_ylabel('Predicted Price (â‚¹)', fontsize=12)\n            ax1.set_title('Actual vs Predicted Prices', fontsize=14, fontweight='bold')\n            ax1.grid(True, alpha=0.3)\n            \n            # 2. Error Distribution\n            ax2 = axes[0, 1]\n            errors = y_pred - y_test\n            ax2.hist(errors, bins=30, color=self.colors['secondary'], alpha=0.7, edgecolor='black')\n            ax2.axvline(x=0, color='red', linestyle='--', linewidth=2)\n            ax2.set_xlabel('Prediction Error (â‚¹)', fontsize=12)\n            ax2.set_ylabel('Frequency', fontsize=12)\n            ax2.set_title('Error Distribution', fontsize=14, fontweight='bold')\n            ax2.grid(True, alpha=0.3)\n            \n            # 3. Time Series Comparison\n            ax3 = axes[1, 0]\n            test_dates = pd.to_datetime(test_data['Date'])\n            ax3.plot(test_dates, y_test, label='Actual', color=self.colors['primary'], linewidth=2)\n            ax3.plot(test_dates, y_pred, label='Predicted', color=self.colors['danger'], linewidth=2, alpha=0.8)\n            ax3.set_xlabel('Date', fontsize=12)\n            ax3.set_ylabel('Price (â‚¹)', fontsize=12)\n            ax3.set_title('Time Series Comparison', fontsize=14, fontweight='bold')\n            ax3.legend()\n            ax3.grid(True, alpha=0.3)\n            fig.autofmt_xdate()\n            \n            # 4. Residual Plot\n            ax4 = axes[1, 1]\n            ax4.scatter(y_pred, errors, alpha=0.6, color=self.colors['success'], s=50)\n            ax4.axhline(y=0, color='red', linestyle='--', linewidth=2)\n            ax4.set_xlabel('Predicted Price (â‚¹)', fontsize=12)\n            ax4.set_ylabel('Residuals (â‚¹)', fontsize=12)\n            ax4.set_title('Residual Plot', fontsize=14, fontweight='bold')\n            ax4.grid(True, alpha=0.3)\n            \n            plt.tight_layout()\n            \n            # Save and display\n            filename = f'{stock_name}_performance_dashboard.png'\n            plt.savefig(filename, dpi=300, bbox_inches='tight', facecolor='white')\n            print(f\"  ðŸ’¾ Performance dashboard saved as: {filename}\")\n            plt.show()  # Display the dashboard\n            \n            return fig\n            \n        except Exception as e:\n            print(f\"  âŒ Performance analysis failed: {str(e)}\")\n            return None\n    \n    def create_summary_visualization(self, all_predictions):\n        \"\"\"Create a summary visualization of all predictions\"\"\"\n        if not all_predictions:\n            return None\n        \n        # Prepare summary data\n        summary_data = []\n        \n        for stock_name, pred_data in all_predictions.items():\n            if pred_data['dynamic'] is not None:\n                current_price = pred_data['current_price']\n                pred_prices = pred_data['dynamic']['Predicted_Price']\n                \n                final_price = pred_prices.iloc[-1]\n                change_pct = ((final_price - current_price) / current_price) * 100\n                \n                summary_data.append({\n                    'Stock': stock_name,\n                    'Current': current_price,\n                    'Predicted': final_price,\n                    'Change%': change_pct\n                })\n        \n        if not summary_data:\n            return None\n        \n        # Create figure\n        fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(16, 8))\n        fig.suptitle('NSE Stock Predictions - Summary Dashboard', fontsize=18, fontweight='bold')\n        \n        # Sort by change percentage\n        summary_df = pd.DataFrame(summary_data)\n        summary_df = summary_df.sort_values('Change%', ascending=True)\n        \n        # 1. Bar chart of percentage changes\n        colors = [self.colors['success'] if x > 0 else self.colors['danger'] \n                  for x in summary_df['Change%']]\n        bars = ax1.barh(summary_df['Stock'], summary_df['Change%'], color=colors, alpha=0.8)\n        ax1.axvline(x=0, color='black', linewidth=1)\n        ax1.set_xlabel('Expected 7-Day Change (%)', fontsize=14, fontweight='bold')\n        ax1.set_title('Predicted Price Changes', fontsize=16, fontweight='bold')\n        ax1.grid(True, alpha=0.3)\n        \n        # Add value labels\n        for bar, value in zip(bars, summary_df['Change%']):\n            x_pos = bar.get_width()\n            ax1.text(x_pos + (0.1 if x_pos > 0 else -0.1), bar.get_y() + bar.get_height()/2,\n                     f'{value:.1f}%', ha='left' if x_pos > 0 else 'right', va='center',\n                     fontweight='bold', fontsize=10)\n        \n        # 2. Current vs Predicted prices - FIXED FOR BETTER DISTINCTION\n        x_pos = np.arange(len(summary_df))\n        width = 0.35\n        \n        # Use contrasting colors and different styles\n        bars1 = ax2.bar(x_pos - width/2, summary_df['Current'], width, \n                        label='Current Price', color='#1f77b4', alpha=0.8, \n                        edgecolor='black', linewidth=1.5)\n        bars2 = ax2.bar(x_pos + width/2, summary_df['Predicted'], width,\n                        label='Predicted Price', color='#ff7f0e', alpha=0.8, \n                        edgecolor='black', linewidth=1.5, hatch='///')\n        \n        # Add value labels on bars to show clear differences\n        for bar, value in zip(bars1, summary_df['Current']):\n            height = bar.get_height()\n            ax2.text(bar.get_x() + bar.get_width()/2., height + height*0.01,\n                     f'â‚¹{value:.0f}', ha='center', va='bottom', fontweight='bold', fontsize=9)\n        \n        for bar, value in zip(bars2, summary_df['Predicted']):\n            height = bar.get_height()\n            ax2.text(bar.get_x() + bar.get_width()/2., height + height*0.01,\n                     f'â‚¹{value:.0f}', ha='center', va='bottom', fontweight='bold', fontsize=9)\n        \n        ax2.set_xlabel('Stocks', fontsize=14, fontweight='bold')\n        ax2.set_ylabel('Price (â‚¹)', fontsize=14, fontweight='bold')\n        ax2.set_title('Current vs 7-Day Predicted Prices', fontsize=16, fontweight='bold')\n        ax2.set_xticks(x_pos)\n        ax2.set_xticklabels(summary_df['Stock'], rotation=45)\n        ax2.legend(fontsize=12, loc='upper left')\n        ax2.grid(True, alpha=0.3)\n        \n        plt.tight_layout()\n        \n        # Save and display\n        filename = 'NSE_Prediction_Summary.png'\n        plt.savefig(filename, dpi=300, bbox_inches='tight', facecolor='white')\n        print(f\"  ðŸ’¾ Summary dashboard saved as: {filename}\")\n        plt.show()  # Display the summary\n        \n        return fig\n\nclass DynamicFeatureEngineer:\n    \"\"\"Enhanced feature engineering with dynamic update capability\"\"\"\n    \n    def add_technical_features(self, df):\n        \"\"\"Add essential technical features\"\"\"\n        df = df.copy()\n        \n        # Moving averages\n        df['MA_5'] = df['Close'].rolling(window=5).mean()\n        df['MA_10'] = df['Close'].rolling(window=10).mean()\n        df['MA_20'] = df['Close'].rolling(window=20).mean()\n        \n        # Price ratios\n        df['MA_5_ratio'] = df['Close'] / df['MA_5']\n        df['MA_20_ratio'] = df['Close'] / df['MA_20']\n        \n        # Returns\n        df['Return_1d'] = df['Close'].pct_change()\n        df['Return_5d'] = df['Close'].pct_change(5)\n        \n        # Volatility\n        df['Volatility_10d'] = df['Return_1d'].rolling(10).std()\n        \n        # Price features\n        df['High_Low_ratio'] = df['High'] / df['Low']\n        df['Close_Open_ratio'] = df['Close'] / df['Open']\n        \n        # Volume features\n        df['Volume_MA_10'] = df['Volume'].rolling(10).mean()\n        df['Volume_ratio'] = df['Volume'] / df['Volume_MA_10']\n        \n        # Lag features\n        for lag in [1, 2, 3]:\n            df[f'Close_lag_{lag}'] = df['Close'].shift(lag)\n        \n        # Drop NaN values\n        df = df.dropna()\n        \n        return df\n    \n    def update_features_with_prediction(self, df, new_price, new_volume=None):\n        \"\"\"Update features with a new predicted price\"\"\"\n        df = df.copy()\n        \n        # Get the last date and add one day\n        last_date = pd.to_datetime(df['Date'].iloc[-1])\n        new_date = last_date + timedelta(days=1)\n        \n        # Use average volume if not provided\n        if new_volume is None:\n            new_volume = df['Volume'].tail(10).mean()\n        \n        # Create new row with predicted values\n        new_row = {\n            'Date': new_date,\n            'Open': new_price,\n            'High': new_price * 1.01,\n            'Low': new_price * 0.99,\n            'Close': new_price,\n            'Volume': new_volume,\n            'Symbol': df['Symbol'].iloc[0]\n        }\n        \n        # Add the new row\n        new_df = pd.concat([df, pd.DataFrame([new_row])], ignore_index=True)\n        \n        # Recalculate features\n        return self.add_technical_features(new_df)\n\nclass ImprovedModelTrainer:\n    \"\"\"Enhanced model trainer with better algorithms\"\"\"\n    \n    def __init__(self):\n        self.models = {}\n        self.scalers = {}\n        self.feature_columns = []\n        self.feature_engineer = DynamicFeatureEngineer()\n        self.all_model_results = {}  # Store all model results for comparison\n    \n    def prepare_features(self, df):\n        \"\"\"Prepare features for training\"\"\"\n        exclude_cols = ['Date', 'Close', 'Symbol']\n        feature_cols = [col for col in df.columns \n                       if col not in exclude_cols and df[col].dtype in ['float64', 'int64']]\n        \n        X = df[feature_cols].fillna(df[feature_cols].mean())\n        y = df['Close']\n        \n        self.feature_columns = feature_cols\n        return X, y\n    \n    def train_models(self, stock_name, df):\n        \"\"\"Train models for a stock\"\"\"\n        print(f\"  ðŸ¤– Training models for {stock_name}\")\n        \n        try:\n            # Prepare data\n            X, y = self.prepare_features(df)\n            \n            # Split data\n            split_idx = int(len(X) * config.TRAIN_TEST_SPLIT)\n            X_train, X_test = X.iloc[:split_idx], X.iloc[split_idx:]\n            y_train, y_test = y.iloc[:split_idx], y.iloc[split_idx:]\n            \n            # Scale features\n            scaler = StandardScaler()\n            X_train_scaled = scaler.fit_transform(X_train)\n            X_test_scaled = scaler.transform(X_test)\n            \n            # Train models with better parameters\n            models = {\n                'Random_Forest': RandomForestRegressor(\n                    n_estimators=100, \n                    max_depth=10,\n                    random_state=42,\n                    n_jobs=-1\n                ),\n                'Linear_Regression': LinearRegression()\n            }\n            \n            best_model = None\n            best_rmse = float('inf')\n            best_name = None\n            model_results = {}\n            \n            for name, model in models.items():\n                # Train\n                model.fit(X_train_scaled, y_train)\n                \n                # Predict\n                y_pred = model.predict(X_test_scaled)\n                \n                # Evaluate\n                rmse = np.sqrt(mean_squared_error(y_test, y_pred))\n                r2 = r2_score(y_test, y_pred)\n                \n                print(f\"    {name}: RMSE={rmse:.2f}, RÂ²={r2:.3f}\")\n                \n                # Store all results\n                model_results[name] = {\n                    'rmse': rmse,\n                    'r2': r2,\n                    'model': model\n                }\n                \n                if rmse < best_rmse:\n                    best_rmse = rmse\n                    best_model = model\n                    best_name = name\n            \n            # Save best model\n            self.models[stock_name] = best_model\n            self.scalers[stock_name] = scaler\n            self.all_model_results[stock_name] = model_results\n            \n            print(f\"    âœ… Best model: {best_name}\")\n            \n            return {\n                'model': best_model,\n                'scaler': scaler,\n                'rmse': best_rmse,\n                'model_name': best_name,\n                'all_results': model_results\n            }\n            \n        except Exception as e:\n            print(f\"    âŒ Training failed: {str(e)}\")\n            return None\n\nclass DynamicPredictor:\n    \"\"\"Enhanced predictor with dynamic feature updates\"\"\"\n    \n    def __init__(self, trainer):\n        self.trainer = trainer\n    \n    def predict_next_days(self, stock_name, df, days=7):\n        \"\"\"Predict next few days with dynamic feature updates\"\"\"\n        if stock_name not in self.trainer.models:\n            print(f\"âŒ No model available for {stock_name}\")\n            return None\n        \n        try:\n            model = self.trainer.models[stock_name]\n            scaler = self.trainer.scalers[stock_name]\n            feature_cols = self.trainer.feature_columns\n            \n            # Start with original dataframe\n            current_df = df.copy()\n            predictions = []\n            dates = []\n            \n            # Generate predictions iteratively\n            for day in range(days):\n                # Get latest features\n                latest_features = current_df[feature_cols].iloc[-1:].fillna(current_df[feature_cols].mean())\n                \n                # Scale and predict\n                X_scaled = scaler.transform(latest_features)\n                predicted_price = model.predict(X_scaled)[0]\n                \n                # Add some controlled randomness to avoid identical predictions\n                if day > 0:  # Don't add noise to first prediction\n                    noise_factor = 0.005  # 0.5% noise\n                    noise = np.random.normal(0, predicted_price * noise_factor)\n                    predicted_price += noise\n                \n                predictions.append(predicted_price)\n                \n                # Create future date\n                last_date = pd.to_datetime(current_df['Date'].iloc[-1])\n                future_date = last_date + timedelta(days=day+1)\n                dates.append(future_date)\n                \n                # Update dataframe with predicted price for next iteration\n                if day < days - 1:  # Don't update on last iteration\n                    current_df = self.trainer.feature_engineer.update_features_with_prediction(\n                        current_df, predicted_price\n                    )\n            \n            # Create prediction dataframe\n            pred_df = pd.DataFrame({\n                'Date': dates,\n                'Predicted_Price': predictions,\n                'Stock': stock_name\n            })\n            \n            return pred_df\n            \n        except Exception as e:\n            print(f\"âŒ Prediction failed for {stock_name}: {str(e)}\")\n            return None\n\n# Main execution\nprint(\"ðŸš€ STARTING ENHANCED NSE PREDICTION SYSTEM WITH SMART CACHING\")\nprint(\"=\" * 70)\n\n# Step 1: Collect data with caching\ncollector = SmartDataCollector()\nstock_data = collector.collect_stock_data()\n\nif not stock_data:\n    print(\"âŒ No data available. Please check internet connection or wait for cache to be created.\")\nelse:\n    print(f\"\\nâœ… Successfully loaded data for {len(stock_data)} stocks\")\n    \n    # Step 2: Feature engineering\n    print(\"\\nðŸ”§ Engineering features...\")\n    engineer = DynamicFeatureEngineer()\n    \n    feature_data = {}\n    for stock_name, df in stock_data.items():\n        try:\n            feature_data[stock_name] = engineer.add_technical_features(df)\n            print(f\"  âœ… {stock_name}: {len(feature_data[stock_name])} rows after feature engineering\")\n        except Exception as e:\n            print(f\"  âŒ Feature engineering failed for {stock_name}: {str(e)}\")\n    \n    # Step 3: Train models\n    print(f\"\\nðŸ¤– Training models...\")\n    trainer = ImprovedModelTrainer()\n    \n    training_results = {}\n    for stock_name, df in feature_data.items():\n        result = trainer.train_models(stock_name, df)\n        if result:\n            training_results[stock_name] = result\n    \n    # Step 3.5: Create model performance comparison (FIXED!)\n    print(f\"\\nðŸ“Š Creating model performance comparison...\")\n    visualizer = ModernVisualizer()\n    if training_results:\n        visualizer.compare_model_performance(training_results)\n    \n    # Step 4: Generate predictions\n    print(f\"\\nðŸ”® Generating predictions...\")\n    \n    predictor = DynamicPredictor(trainer)\n    \n    # Store all predictions for visualization\n    all_predictions = {}\n    \n    print(\"\\n\" + \"=\" * 80)\n    print(\"ðŸ“Š PREDICTION RESULTS WITH MODERN VISUALIZATIONS\")\n    print(\"=\" * 80)\n    \n    for st